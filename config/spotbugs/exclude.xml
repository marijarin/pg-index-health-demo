<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Bug pattern="EI_EXPOSE_REP,RCN_REDUNDANT_NULLCHECK_WOULD_HAVE_BEEN_A_NPE,SQL_NONCONSTANT_STRING_PASSED_TO_EXECUTE,BC_UNCONFIRMED_CAST_OF_RETURN_VALUE,SLF4J_LOGGER_SHOULD_BE_NON_STATIC,WEM_WEAK_EXCEPTION_MESSAGING"/>
    </Match>
    <Match>
        <Bug pattern="RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE"/>
        <Class name="~.*Test"/>
    </Match>
    <Match>
        <Bug pattern="MDM_THREAD_YIELD"/>
        <Class name="io.github.mfvanek.pg.index.health.demo.utils.StatisticsCollector"/>
    </Match>
    <Match>
        <Bug pattern="SIL_SQL_IN_LOOP"/>
        <Class name="io.github.mfvanek.pg.index.health.demo.utils.MigrationsGenerator"/>
    </Match>
    <Match>
        <Bug pattern="CRLF_INJECTION_LOGS,SQL_INJECTION_JDBC"/>
    </Match>
</FindBugsFilter>
